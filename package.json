{
  "name": "VAST-Tester",
  "version": "0.0.0",
  "description": "Tests IAB VAST ads.",
  "private": true,
  "homepage": "https://vasttester.iabtechlab.com/",
  "repository": "InteractiveAdvertisingBureau/VAST-Tester",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8",
    "yarn": ">=1.7.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf build",
    "build": "yarn run clean && REACT_APP_BUILD_DATE=\"$( TZ=GMT date )\" react-scripts build",
    "test": "standard \"src/**/*.js\"",
    "format": "prettier-standard \"src/**/*.js\" && standard --fix \"src/**/*.js\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "base16": "^1.0.0",
    "debug": "^4.1.1",
    "fecha": "^3.0.2",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "husky": "^1.3.1",
    "iab-vast-loader": "^2.0.0",
    "lint-staged": "^8.1.0",
    "lodash-es": "^4.17.11",
    "lower-case-first": "^1.0.2",
    "node-sass": "^4.11.0",
    "p-defer": "^1.0.0",
    "p-timeout": "^2.0.1",
    "prettier-standard": "^9.1.1",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react": "^16.7.0",
    "react-collapsible": "^2.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-expanding-textarea": "^0.2.0",
    "react-fontawesome": "^1.6.1",
    "react-json-tree": "^0.11.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-tabs": "^3.0.0",
    "react-toggle": "^4.0.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reset-css": "^4.0.1",
    "rimraf": "^2.6.3",
    "rxjs": "^6.3.3",
    "serialize-error": "^3.0.0",
    "standard": "^12.0.1",
    "upper-case-first": "^1.1.2",
    "url-search-params": "^1.1.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard",
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "Blob"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
